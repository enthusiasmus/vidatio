#visualization
    margin 0px auto 30px auto
    .content-area
        height 500px
        padding 25px
        border-top 3px solid $color-red
        background $color-white
        box-shadow $box-shadow-size $color-shadow

        .angular-leaflet-map
            height 100%
            width 100%

            a:link, a:visited
                color $color-black
