#editor.wrapper-content(ng-controller="EditorCtrl")
    .editor-panel
        //- TODO: -needs to be moved from HeaderCtrl to EditorCtrl
        //-       -third tab
        //-       -only one active at a time
        a.tab(href='#' ng-class="{'active': activeViews[0]}" ng-click="tabClicked(0)") {{ "DATASET" | translate | uppercase }}
        a.tab.active.middle(href='#' ng-class="{'active': activeViews[1]}" ng-click="tabClicked(1)") {{ "DATASET + VISUALIZATION" | translate | uppercase }}
        a.tab(href='#' ng-class="{'active': activeViews[2]}" ng-click="tabClicked(2)") {{ "VISUALIZATION" | translate | uppercase }}
    .row
        include table/table
        include visualization/visualization
