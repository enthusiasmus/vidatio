        padding 20px
        border-top 3px solid #11ddc6
        background #fff
        box-shadow 0 2px 9px #e3e3e3
        margin 0 auto
        color #333
    h1
        margin-bottom 20px

    .team p
        margin 0

    p
    address
        line-height 1.5

    .disclaimer
        p
            line-height 1.5
