#imprint
    .area
        padding 20px
        border-top 3px solid $color-green
        background $color-white
        box-shadow 0 2px 9px $color-shadow
        margin 0 auto 20px
        color $color-grey-dark
    h1
        margin-bottom 20px

    .team p
        margin 0

    p
    address
        line-height 1.5

    .disclaimer
        p
            line-height 1.5
            color $color-passive

    .logos
        min-height 100px
        height auto
        width 100%
        margin 30px 0 50px

        .logo-fh
        .logo-mmt
            float left
            height 90px
            margin-top 20px

        .logo-fh
            margin-left 20px
            margin-bottom 20px



