    .logos
        min-height 100px
        height auto
        width 100%
        margin 30px 0 50px

        .logo-fh
        .logo-mmt
            float left
            height 90px
            margin-top 20px

        .logo-fh
            margin-left 20px
            margin-bottom 20px



